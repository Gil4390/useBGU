MLM ABCD

  model AB
    class A
      attributes
        a1: String
        a2: String
    end

    class B
      attributes
        b1: String
        b2: String
    end

    association ab1 between
      A[*] role aa1
      B[*] role bb1
    end


  model CD
    class C
      attributes
        c: String
    end

    class D
      attributes
        d: String
    end

    association cd1 between
      C[*] role cc1
      D[*] role dd1
    end


mediator AB < NONE
end

mediator CD < AB
   clabject C : A
   end

   clabject D : B
   end

   assoclink cd1 : ab1
      role_renaming
        aa1 -> aa1
        bb1 -> bb1
   end
end
