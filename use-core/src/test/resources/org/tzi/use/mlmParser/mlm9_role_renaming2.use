MLM ABCD

  model AB
    class A
      attributes
        a1: String
        a2: Integer
    end

    class B
      attributes
        b1: String
        b2: String
    end

    association ab1 between
      A[*] role aa1
      B[*] role bb1
    end


  model CD
    class C
      attributes
        c: String
    end

    class D
      attributes
        d: String
    end

    association cd1 between
      C[*] role cc1
      D[*] role dd1
    end

    constraints
    context C
      inv GG:
        self.dd1->forAll( it | it.d = 'd')


inter-constraints
context CD@C
  inv GG2:
    self.bb1->forAll( it | it.b1 = 'b')

mediator AB < NONE
end

mediator CD < AB
   clabject C : A
   end

   clabject D : B
   end

end

