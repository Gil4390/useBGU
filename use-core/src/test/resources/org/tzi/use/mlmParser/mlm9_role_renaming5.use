MLM ABCD

  model AB
    class A
    end

    class B
    end

    association ab1 between
      A[*] role aa
      B[*] role bb
    end


  model CD
    class C
    end

    class D
    end

    association cd1 between
      C[*] role cc
      D[*] role dd
    end

  model EF
    class E
    end

    class F
    end

    association ef1 between
      E[*] role ee
      F[*] role ff
    end



mediator AB < NONE
end

mediator CD < AB
   clabject C : A
   end

   clabject D : B
   end

   assoclink cd1 : ab1
      role_renaming
        bb -> x
   end
end

mediator EF < CD
   clabject E : C
   end

   clabject F : D
   end

   assoclink ef1 : cd1
      role_renaming
        x -> y
   end
end
