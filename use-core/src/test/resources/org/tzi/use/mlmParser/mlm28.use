MLM ABCD

  model AB
    class A
      attributes
        a1: String
        a2: Integer
    end

    class B
      attributes
        b1: String
        b2: String
    end

    association ab1 between
      A[*] role aa1
      B[*] role bb1
    end


  model CD
    class C
      attributes
        c: String
    end

    class D
      attributes
        d: String
    end

    association cd1 between
      C[*] role cc1
      D[*] role dd1
    end

  model EF
    class E
      attributes
        e: String
    end

    class F
      attributes
        f: String
    end

    association ef1 between
      E[*] role ee1
      F[*] role ff1
    end


inter-constraints
context EF@E
  inv i1:
    self.e9 = 'e'
  inv i2:
    self.bb9->forAll( it | it.b1 = 'b')

context CD@C
  inv i3:
    self.c5 = 'c'
  inv i4:
    self.bb5->forAll( it | it.b1 = 'b')


mediator AB < NONE
end

mediator CD < AB
  clabject C : A
    attributes
      a1 -> c5
    roles
      bb1 -> bb5
  end

  clabject D : B
  end
end

mediator EF < CD
  clabject E : C
    attributes
      c5 -> e9
    roles
      bb5 -> bb9
  end

  clabject F : D
  end
end

