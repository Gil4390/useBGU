MLM ABCD

  model AB
    class A
      attributes
        a1: String
    end

    class B
      attributes
        b1: String
    end

    association ab1 between
      A[*] role aa1
      B[*] role bb1
    end


  model CD
    class C
    end

    class D
    end

    association cd1 between
      C[*] role cc1
      D[*] role dd1
    end

  model EF
      class E
      end

      class F
      end

      association ef1 between
        E[*] role ee1
        F[*] role ff1
      end
inter-constraints
context EF@E inv:
    self.bb1->forAll(b | b.b1 = 'test')

mediator AB < NONE
end

mediator CD < AB
    clabject C : A
        roles
            ~bb1
    end

    clabject D : B
    end

end

mediator EF < CD
    clabject E : C
    end

    clabject F : D
    end


end